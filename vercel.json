{
  "builds": [
    {
      "src": "clothify/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.12.6"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "clothify/wsgi.py"
    }
  ],
  "functions": {
    "api/**": {
      "excludeFiles": "{.next,*.cache,node_modules,public,app}/**"
    }
  }
}
